/*
вывод только цифр и символов
Элемент № такой то - является числом
Элемент № такой-то не число
*/
#include "pch.h"
#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int i;
	char sym[10] = { 'A','1','B','2','C','3','D','4','E','5' };
	for (i = 0; i <= 9; i++)
		
		if (sym[i] == '1' || sym[i] == '2' || sym[i] == '3' || sym[i] == '4' || sym[i] == '5' || sym[i] == '6' || sym[i] == '7' || sym[i] == '8' || sym[i] == '9') 
		{	cout << " Элемент № " << i << " Число " << "\n";}		
		
		else { cout << " Элемент № " << i << " Символ " << "\n";}

}



